<h1>Listing Orders</h1>



<% if @orders.present? %>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.shipping_id %></td>
        <td><%= order.shipping_fee %></td>
        <td><%= link_to "Show", admin_order_path(order.id) %></td>
        <td><%= link_to "Edit", edit_admin_order_path(order.id) %></td>
        <td><%= link_to "Destroy", admin_order_path(order.id), method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% end %>

<br>
 
<%= link_to "New order", new_admin_order_path %>