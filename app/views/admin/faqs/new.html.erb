<div class="container-fluid">
    <div class="page-header">
        <div class="row">
            <div class="col-6">
                <h3>FAQ</h3>
                <ol class="breadcrumb">
                <li class="breadcrumb-item">
                	<%= link_to admin_path do %>
                      <i data-feather="home"></i>
                    <% end %> 
                </li>
                <li class="breadcrumb-item">FAQ</li>
              </ol>
            </div>
        </div>
    </div>
</div>

 
<div class="row">
	<div class="col-sm-12">
		<div class="card">
			<div class="card-header">
				<h5>FAQ</h5>
			</div>
			<div class="card-body">
				<%= form_for @faq,url: {action: "create"},html: {class: "theme-form"} do |f| %>
					<div class="form-group">
						<label class="col-form-label pt-0" for="title">Question</label>
						<%= f.text_field "question", class: 'form-control' %>
					</div>
					<div class="form-group">
						<label class="col-form-label pt-0" for="title">Answer</label>
						<%= f.text_area "answer",class: 'form-control' %>
					</div>
					<div class="card-footer">
						<input type="submit" value="Submit" class="btn btn-primary"/>
						<%= link_to "Cancel", admin_faqs_path, class: "btn btn-secondary" %>
					</div>
				<% end %>
			</div>
		</div>
	</div>
</div>
 
